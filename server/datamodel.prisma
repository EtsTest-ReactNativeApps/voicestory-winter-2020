type User {
  id: ID! @id
  email: String @unique
  first_name: String!
  last_name: String !
}

type Speaker {
  id: ID! @id
  profile_picture: String
  titles: [String]!
  description: String!
  social_media: [String!]!
  videos: [Video!]!

}

type Video {
  id: ID! @id
  title: String!
  url: String! @default(value: "www.youtube.com")
  author: Speaker @relation(link: INLINE)
}